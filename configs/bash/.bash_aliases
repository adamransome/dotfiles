# .bash_aliases

alias ll='ls -alF'
alias la='ls -A'
alias l='ls -CF'
alias :q='exit'
alias ws='cd ~/workspace'
alias scripts='cd /usr/local/bin/scripts'
alias pbcopy="xclip -sel clip"
alias tmux='tmux -2'
alias glogs="git log --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit"
